{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "rootDir": "src",
    "outDir": "dist",
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "plugins": [{"transform": "typia/lib/transform"}],
    "strictNullChecks": true
  }
}
